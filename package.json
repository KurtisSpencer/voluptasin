{
  "name": "voluptasin",
  "version": "1.6.2",
  "description": "Ethereum Wallet",
  "author": "SigurdGutmann",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "vitae",
    "quis",
    "ethereum",
    "blockchain"
  ],
  "devDependencies": {
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "generator-node": "^2.3.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KurtisSpencer/voluptasin.git"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KurtisSpencer/voluptasin.git/issues"
  }
}
